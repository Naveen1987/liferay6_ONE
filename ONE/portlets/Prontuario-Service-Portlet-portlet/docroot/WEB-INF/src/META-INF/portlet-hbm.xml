<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.daffo.model.prontuario_drug" />
	<class name="com.liferay.daffo.model.impl.prontuario_drugImpl" table="prontuario_drug">
		<id name="drug_id" type="long">
			<generator class="assigned" />
		</id>
		<property name="Gruppo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="SottoGruppo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Codice" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Principio_Attivo" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Nome_Commerciale" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>