<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.daffo.model.News_Blogs" />
	<class name="com.liferay.daffo.model.impl.News_BlogsImpl" table="News_Blogs">
		<id name="News_Blogs_Id" type="long">
			<generator class="assigned" />
		</id>
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="body" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Publish_Date" type="org.hibernate.type.TimestampType" />
		<property name="Expiration_Date" type="org.hibernate.type.TimestampType" />
		<property name="author" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="visible" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>