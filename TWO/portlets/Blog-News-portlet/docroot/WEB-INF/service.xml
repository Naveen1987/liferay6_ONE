<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">
<service-builder package-path="com.liferay.daffo">
	<author>Naveen Kumar</author>
	<namespace>NewsBlogs</namespace>
	<entity name="News_Blogs" cache-enabled="false" table="News_Blogs" local-service="true" remote-service="false">
        <column name="News_Blogs_Id" type="long" primary="true"></column>
        <column name="title" type="String"></column>
        <column name="body" type="String"></column>
        <column name="Publish_Date" type="Date"></column>
        <column name="Expiration_Date" type="Date"></column>
        <column name="author" type="String"></column>
        <column name="visible" type="String"></column>
        
    </entity>
    
</service-builder>